<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_userpage"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">

    <RelativeLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <RelativeLayout
                android:id="@+id/rv_papan_jaringan"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/rounded_shape3"
                android:padding="10dp">

                <ImageView
                    android:id="@+id/user_image_board"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="3dp"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />

                <ImageView
                    android:id="@+id/d1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="60dp"
                    android:layout_marginTop="40dp"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />

                <ImageView
                    android:id="@+id/d2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="60dp"
                    android:layout_marginTop="40dp"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />

                <ImageView
                    android:id="@+id/d3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="60dp"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />

                <ImageView
                    android:id="@+id/d4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="60dp"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />

                <ImageView
                    android:id="@+id/d5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="100dp"
                    android:layout_marginTop="60dp"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />

                <ImageView
                    android:id="@+id/d6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="100dp"
                    android:layout_marginTop="60dp"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />

                <ImageView
                    android:id="@+id/d7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/relativeLayout2"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />

                <ImageView
                    android:id="@+id/d8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="40dp"
                    android:layout_marginTop="100dp"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />

                <ImageView
                    android:id="@+id/d9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="80dp"
                    android:layout_marginTop="100dp"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />

                <ImageView
                    android:id="@+id/d10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="120dp"
                    android:layout_marginTop="100dp"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />

                <ImageView
                    android:id="@+id/d11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="100dp"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />


                <ImageView
                    android:id="@+id/d12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="40dp"
                    android:layout_marginTop="100dp"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />

                <ImageView
                    android:id="@+id/d13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="80dp"
                    android:layout_marginTop="100dp"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />

                <ImageView
                    android:id="@+id/d14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="120dp"
                    android:layout_marginTop="100dp"
                    android:background="@drawable/circular_image_view"
                    android:src="@drawable/ic_user" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/d14"
                    android:layout_marginTop="8dp"
                    android:id="@+id/relativeLayout2">

                    <LinearLayout
                        android:id="@+id/rl_baris_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textview_baris_4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:layout_marginTop="18dp"
                            android:text="Baris 4"
                            android:textAllCaps="true"
                            android:textColor="@android:color/black" />

                        <android.support.v7.widget.RecyclerView
                            android:id="@+id/recyclerview_baris4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                        </android.support.v7.widget.RecyclerView>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/rl_baris_5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/rl_baris_4"
                        android:layout_marginTop="8dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/textview_baris_5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:layout_marginTop="18dp"
                            android:text="Baris 5"
                            android:textAllCaps="true"
                            android:textColor="@android:color/black" />
                        <android.support.v7.widget.RecyclerView
                            android:id="@+id/recyclerview_baris5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                        </android.support.v7.widget.RecyclerView>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/rl_baris_6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/rl_baris_5"
                        android:layout_marginTop="8dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textview_baris_6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:layout_marginTop="18dp"
                            android:text="Baris 6"
                            android:textAllCaps="true"
                            android:textColor="@android:color/black" />
                        <android.support.v7.widget.RecyclerView
                            android:id="@+id/recyclerview_baris6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                        </android.support.v7.widget.RecyclerView>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/rl_baris_7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/rl_baris_6"
                        android:layout_marginTop="8dp"
                        android:orientation="horizontal"
                        android:paddingBottom="8dp">

                        <TextView
                            android:id="@+id/textview_baris_7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:layout_marginTop="18dp"
                            android:text="Baris 7"
                            android:textAllCaps="true"
                            android:textColor="@android:color/black" />
                        <android.support.v7.widget.RecyclerView
                            android:id="@+id/recyclerview_baris7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                        </android.support.v7.widget.RecyclerView>
                    </LinearLayout>
                </RelativeLayout>
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>
